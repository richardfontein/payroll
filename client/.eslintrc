{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "classes": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "commonjs": false,
    "jest": true
  },
  "plugins": ["jsx-a11y", "react", "import", "prettier"],
  "rules": {
    "arrow-parens": "off",
    "implicit-arrow-linebreak": "off",
    "indent": "off",
    "no-trailing-spaces": "off",
    "import/prefer-default-export": "off"
  }
}
